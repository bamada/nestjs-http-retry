{
  "name": "@bamada/nestjs-http-retry",
  "version": "0.1.2",
  "description": "A robust NestJS module providing configurable HTTP request retry strategies, including Exponential Backoff and Constant Interval, to enhance the resilience of your applications.",
  "author": "bamada",
  "private": false,
  "keywords": [
    "nestjs",
    "http",
    "retry",
    "http-client",
    "resilience",
    "retry-strategy",
    "exponential-backoff",
    "interval-retry",
    "robust",
    "networking",
    "module",
    "nestjs-module",
    "fault-tolerant",
    "timeout",
    "resilient-requests",
    "backoff-strategy",
    "error-handling",
    "circuit-breaker",
    "framework",
    "axios-retry",
    "http-requests"
  ],
  "license": "MIT",
  "url": "https://github.com/bamada/nestjs-http-retry#readme",
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "release": "release-it",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-angular": "^19.3.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "all-contributors-cli": "^6.26.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.0.0",
    "reflect-metadata": "^0.2.0",
    "release-it": "^17.2.1",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "**/*.{ts,json}": []
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bamada/nestjs-http-retry"
  }
}
